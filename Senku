int main() {
    int tab;
    int x1,x2,y1,y2;
    tipomatriz tablero1[nrow1][ncol1];
    tipomatriz tablero2[nrow2][ncol2];
    tipomatriz tablero3[nrow3][ncol3];
    crear_tablero1(tablero1,nrow1,ncol1);
    crear_tablero2(tablero2,nrow2,ncol2);
